{
  "name": "qualia_nss",
  "version": "1.0.0",
  "description": "Qualia NSS Project",
  "scripts": {
    "docs:generate": "node docs/scripts/generate-docs.js",
    "docs:serve": "npm run build:wiki && docsify serve src/wiki/public -o",
    "docs:watch": "nodemon --watch src --ext js,jsx,ts,tsx --exec npm run docs:generate",
    "start": "npm run build:wiki && docsify serve src/wiki/public -o",
    "build:wiki": "node scripts/copy-wiki-files.js",
    "dev": "npm run build:wiki && npm run start"
  },
  "devDependencies": {
    "docdash": "^1.2.0",
    "docsify-cli": "^4.4.4",
    "fs-extra": "^11.3.1",
    "jsdoc": "^4.0.0",
    "nodemon": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nyrk0/qualia_nss.git"
  },
